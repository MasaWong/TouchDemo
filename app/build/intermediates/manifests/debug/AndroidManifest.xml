<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="me.chunyu.demo"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="20" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="me.chunyu.demo.MyActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="me.chunyu.demo.TouchEvent.TouchEventActivity"
            android:label="@string/title_activity_touch_event" >
        </activity>
        <activity
            android:name="me.chunyu.demo.DragEvent.DragEventActivity"
            android:label="@string/title_activity_drag" >
        </activity>
        <activity
            android:name="me.chunyu.demo.SortEvent.SortEventActivity"
            android:label="@string/title_activity_sort_event" >
        </activity>
        <activity
            android:name="me.chunyu.demo.SelectEvent.SelectEventActivity"
            android:label="@string/title_activity_select_event" >
        </activity>
        <activity
            android:name="me.chunyu.demo.PileupEvent.PileupEventActivity"
            android:label="@string/title_activity_pileup" >
        </activity>
    </application>

</manifest>