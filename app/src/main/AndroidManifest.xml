<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="me.chunyu.demo" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MyActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".TouchEvent.TouchEventActivity"
            android:label="@string/title_activity_touch_event" >
        </activity>
        <activity
            android:name=".DragEvent.DragEventActivity"
            android:label="@string/title_activity_drag" >
        </activity>
        <activity
            android:name=".SortEvent.SortEventActivity"
            android:label="@string/title_activity_sort_event" >
        </activity>
        <activity
            android:name=".SelectEvent.SelectEventActivity"
            android:label="@string/title_activity_select_event" >
        </activity>
        <activity
            android:name=".PileupEvent.PileupEventActivity"
            android:label="@string/title_activity_pileup" >
        </activity>
    </application>

</manifest>
